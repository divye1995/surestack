{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "baseapp": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "root": "projects/baseapp",
      "sourceRoot": "projects/baseapp/src",
      "prefix": "ng-sure",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/baseapp",
            "index": "projects/baseapp/src/index.html",
            "main": "projects/baseapp/src/main.ts",
            "polyfills": "projects/baseapp/src/polyfills.ts",
            "tsConfig": "projects/baseapp/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/baseapp/src/favicon.ico",
              "projects/baseapp/src/assets"
            ],
            "styles": [
              "projects/baseapp/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/baseapp/src/environments/environment.ts",
                  "with": "projects/baseapp/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "baseapp:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "baseapp:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "baseapp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/baseapp/src/test.ts",
            "polyfills": "projects/baseapp/src/polyfills.ts",
            "tsConfig": "projects/baseapp/tsconfig.spec.json",
            "karmaConfig": "projects/baseapp/karma.conf.js",
            "assets": [
              "projects/baseapp/src/favicon.ico",
              "projects/baseapp/src/assets"
            ],
            "styles": [
              "projects/baseapp/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/baseapp/tsconfig.app.json",
              "projects/baseapp/tsconfig.spec.json",
              "projects/baseapp/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/baseapp/e2e/protractor.conf.js",
            "devServerTarget": "baseapp:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "baseapp:serve:production"
            }
          }
        }
      }
    },
    "threedview": {
      "projectType": "library",
      "root": "projects/threedview",
      "sourceRoot": "projects/threedview/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/threedview/tsconfig.lib.json",
            "project": "projects/threedview/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/threedview/src/test.ts",
            "tsConfig": "projects/threedview/tsconfig.spec.json",
            "karmaConfig": "projects/threedview/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/threedview/tsconfig.lib.json",
              "projects/threedview/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "playground": {
      "root": "projects/baseapp",
      "sourceRoot": "projects/baseapp/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/playground",
            "index": "projects/baseapp/src/index.html",
            "main": "projects/baseapp/src/main.playground.ts",
            "polyfills": "projects/baseapp/src/polyfills.ts",
            "tsConfig": "projects/baseapp/tsconfig.playground.json",
            "assets": [
              "projects/baseapp/src/favicon.ico",
              "projects/baseapp/src/assets"
            ],
            "styles": [
              "projects/baseapp/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/baseapp/src/environments/environment.ts",
                  "with": "projects/baseapp/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "playground:build",
            "port": 4201
          }
        }
      }
    }
  },
  "cli": {
    "packageManager": "yarn"
  },
  "defaultProject": "baseapp"
}